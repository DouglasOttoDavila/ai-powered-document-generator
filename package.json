{
  "name": "ai-powered-document-generator",
  "displayName": "AI-Powered Document Generator",
  "description": "Generate intelligent documentation for any code using Gemini API.",
  "publisher": "DouglasDavila",
  "version": "0.0.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/DouglasOttoDavila/ai-powered-document-generator"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-documentation",
          "title": "AI-Powered Doc Generator",
          "icon": "./media/icon.png"
        }
      ]
    },
    "views": {
      "ai-documentation": [
        {
          "id": "aiDocGenerator",
          "name": "Beta Version",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "ai-doc-gemini.generateDocs",
        "title": "Generate AI Documentation"
      }
    ],
    "configuration": {
      "title": "AI Documentation Generator",
      "properties": {
        "aiDocGenerator.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Gemini API key for generating documentation"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@types/vscode": "^1.102.0",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "@google/genai": "^1.9.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "html-to-text": "^9.0.5",
    "markitdown": "^0.0.4",
    "xlsx": "^0.18.5"
  }
}
